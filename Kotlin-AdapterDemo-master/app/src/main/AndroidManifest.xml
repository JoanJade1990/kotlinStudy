<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"  package="chy.adapterdemo">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="chy.adapterdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="chy.adapterdemo.ArrayAdapterActivity">
        </activity>
        <activity android:name="chy.adapterdemo.SimpleAdapterActivity">
        </activity>
        <activity android:name="chy.adapterdemo.SimpleCursorAdapterActivity">
        </activity>
        <activity android:name="chy.adapterdemo.BaseAdapterActivity">
        </activity>
    </application>

</manifest>